<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MyPage">

    <select id="getInterestList" resultType="com.lemonmarket.web.dto.MytradeDTO" parameterType="mytradedto">
        SELECT * FROM interest_table WHERE userId = #{userId}
    </select>
    
    <select id="getPurchaseList" resultType="mytradedto" parameterType="string">
        SELECT * FROM purchase_table WHERE userId = #{userId}
    </select>

</mapper>